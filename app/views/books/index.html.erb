<head>
  <title>WebcampBackendNew</title>
</head>

<body>
  <p id="notice"></p>
  <h1>Books</h1>
  <% @books.each do |book| %>
    <p>Title</p>
    <span>
      <%= link_to book.title, book_path(book.id) %>
    </span>
    <p>Body</p>
    <span><%= book.body %></span>
  <% end %>
  <h2>New book</h2>
</body>